<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.workout_manager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Workoutmanager">
        <activity
            android:name=".ActivityForResult_BMI"
            android:exported="false"
            android:label="Calculate BMI"
            android:parentActivityName=".AsyncTask_Calculates"
            />
        <activity
            android:name=".AsyncTask_Calculates"
            android:exported="false"
            android:label="Calculate BMI / BMR"
            android:parentActivityName=".Workouts" />
        <activity
            android:name=".CreateWorkout"
            android:exported="false"
            android:label="Create Workout"
            android:parentActivityName=".Workouts" />
        <activity
            android:name=".Workouts"
            android:exported="false"
            android:label="Workouts"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".AppRegister"
            android:exported="false"
            android:label="Register"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>